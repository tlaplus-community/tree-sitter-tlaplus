{
  "name": "@tlaplus/tree-sitter-tlaplus",
  "version": "0.6.0",
  "description": "A tree-sitter grammar for TLA+ and PlusCal",
  "main": "bindings/node",
  "scripts": {
    "test": "tree-sitter test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tlaplus-community/tree-sitter-tlaplus.git"
  },
  "keywords": [
    "TLA+",
    "PlusCal",
    "tree-sitter",
    "grammar",
    "parser"
  ],
  "author": "Andrew Helwer",
  "contributors": ["Vasiliy Morkovkin"],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tlaplus-community/tree-sitter-tlaplus/issues"
  },
  "homepage": "https://github.com/tlaplus-community/tree-sitter-tlaplus#readme",
  "dependencies": {
    "nan": "^2.14.2"
  },
  "devDependencies": {
    "tree-sitter-cli": "^0.20.6"
  },
  "tree-sitter": [
    {
      "scope": "source.tlaplus",
      "file-types": [
        "tla"
      ],
      "highlights": [
        "queries/highlights.scm"
      ],
      "locals": [
        "queries/locals.scm"
      ]
    }
  ]
}
