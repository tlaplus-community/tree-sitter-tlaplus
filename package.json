{
  "name": "@tlaplus/tree-sitter-tlaplus",
  "version": "1.2.1",
  "description": "A tree-sitter grammar for TLA‚Å∫ and PlusCal",
  "main": "bindings/node",
  "types": "bindings/node",
  "scripts": {
    "test": "npx tree-sitter test",
    "install": "node-gyp-build",
    "prebuildify": "prebuildify --napi --strip"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tlaplus-community/tree-sitter-tlaplus.git"
  },
  "keywords": [
    "tla+",
    "tlaplus",
    "pluscal",
    "tree-sitter",
    "grammar",
    "parser"
  ],
  "author": "Andrew Helwer",
  "contributors": [
    "Vasiliy Morkovkin"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tlaplus-community/tree-sitter-tlaplus/issues"
  },
  "homepage": "https://github.com/tlaplus-community/tree-sitter-tlaplus#readme",
  "dependencies": {
    "node-gyp-build": "^4.8.0",
    "node-addon-api": "^7.1.0"
  },
  "peerDependencies": {
    "tree-sitter-cli": "0.21.0"
  },
  "peerDependenciesMeta": {
    "tree_sitter": {
      "optional": true
    }
  },
  "devDependencies": {
    "prebuildify": "6.0.0",
    "tree-sitter-cli": "0.22.1"
  },
  "files": [
    "grammar.js",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "src/**",
    "*.wasm"
  ],
  "tree-sitter": [
    {
      "scope": "source.tlaplus",
      "file-types": [
        "tla"
      ],
      "highlights": [
        "queries/highlights.scm"
      ],
      "locals": [
        "queries/locals.scm"
      ]
    }
  ]
}
